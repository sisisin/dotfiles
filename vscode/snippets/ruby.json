{
  "each": {
    "body": [".each { |${1:value}|", "$2", "}"],
    "prefix": ".each"
  },
  "each_with_index": {
    "body": [".each_with_index { |${1:value},${2:index}|", "$3", "}"],
    "prefix": ".each_with_index"
  },
  "lambda": {
    "body": ["{ |${1:value}|", "  $2", "}"],
    "prefix": "lam"
  },
  "map": {
    "body": [".map { |${1:value}|", "$2", "}"],
    "prefix": ".map"
  }
}
